<%= render(Primer::Beta::BorderBox.new) do |component| %>
  <% component.with_row do %>
    Default
  <% end %>
  <% component.with_row(scheme: :neutral) do %>
    Neutral
  <% end %>
  <% component.with_row(scheme: :info) do %>
    Info
  <% end %>
  <% component.with_row(scheme: :warning) do %>
    Warning
  <% end %>
<% end %>
